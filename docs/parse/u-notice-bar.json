      }
    ],
    "event": [
      {
        "description": "{Function} click 点击通告文字触发，只有mode = vertical，或者mode = horizontal且is-circular = false时有效",
        "title": "event"
      },
      {
        "description": "{Function} close 点击右侧关闭图标触发",
        "title": "event"
      },
      {
        "description": "{Function} getMore 点击右侧向右图标触发",
        "title": "event"
      },
      {
        "description": "{Function} end 列表的消息每次被播放一个周期时触发，只有mode = vertical，或者mode = horizontal且is-circular = false时有效",
        "title": "event"
      }
    ],
    "examples": [
