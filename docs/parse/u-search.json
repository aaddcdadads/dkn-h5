  "keywords": [
    {
      "name": "description",
      "description": "搜索组件，集成了常见搜索框所需功能，用户可以一键引入，开箱即用。"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/search.html"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tshape\t\t\t\t搜索框形状，round-圆形，square-方形（默认 'round' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tbgColor\t\t\t\t搜索框背景颜色（默认 '#f2f2f2' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tplaceholder\t\t\t占位文字内容（默认 '请输入关键字' ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tclearabled\t\t\t是否启用清除控件（默认 true ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tfocus\t\t\t\t是否自动获得焦点（默认 false ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tshowAction\t\t\t是否显示右侧控件（默认 true ）"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tactionStyle\t\t\t右侧控件的样式，对象形式"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tactionText\t\t\t右侧控件文字（默认 '搜索' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tinputAlign\t\t\t输入框内容水平对齐方式 （默认 'left' ）"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tinputStyle\t\t\t自定义输入框样式，对象形式"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tdisabled\t\t\t是否启用输入框（默认 false ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tborderColor\t\t\t边框颜色，配置了颜色，才会有边框 (默认 'transparent' )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tsearchIconColor\t\t搜索图标的颜色，默认同输入框字体颜色 (默认 '#909399' )"
    },
    {
      "name": "property",
      "description": "{Number | String}\tsearchIconSize 搜索图标的字体，默认22"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tcolor\t\t\t\t输入框字体颜色（默认 '#606266' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tplaceholderColor\tplaceholder的颜色（默认 '#909399' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tsearchIcon\t\t\t输入框左边的图标，可以为uView图标名称或图片路径  (默认 'search' )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tmargin\t\t\t\t组件与其他上下左右元素之间的距离，带单位的字符串形式，如\"30px\"   (默认 '0' )"
    },
    {
      "name": "property",
      "description": "{Boolean} \t\t\tanimation\t\t\t是否开启动画，见上方说明（默认 false ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tvalue\t\t\t\t输入框初始值"
    },
    {
      "name": "property",
      "description": "{String | Number}\tmaxlength\t\t\t输入框最大能输入的长度，-1为不限制长度  (默认 '-1' )"
    },
    {
      "name": "property",
      "description": "{String | Number}\theight\t\t\t\t输入框高度，单位px（默认 64 ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tlabel\t\t\t\t搜索框左边显示内容"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tcustomStyle\t\t\t定义需要用到的外部样式"
    },
    {
      "name": "event",
      "description": "{Function} change 输入框内容发生变化时触发"
    },
    {
      "name": "event",
      "description": "{Function} search 用户确定搜索时触发，用户按回车键，或者手机键盘右下角的\"搜索\"键时触发"
    },
    {
      "name": "event",
      "description": "{Function} custom 用户点击右侧控件时触发"
    },
    {
      "name": "event",
      "description": "{Function} clear 用户点击清除按钮时触发"
    },
    {
      "name": "example",
      "description": "<u-search placeholder=\"日照香炉生紫烟\" v-model=\"keyword\"></u-search>"
    }
  ],
