  "keywords": [
    {
      "name": "description",
      "description": "该组件一般用于页面顶部向下滑出一个提示，尔后自动收起的场景"
    },
    {
      "name": "tutorial"
    },
    {
      "name": "property",
      "description": "{String | Number}\ttop\t\t\t\t\t到顶部的距离 ( 默认 0 )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\ttype\t\t\t\t主题，primary，success，warning，error ( 默认 'primary' )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tcolor\t\t\t\t字体颜色 ( 默认 '#ffffff' )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tbgColor\t\t\t\t背景颜色"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tmessage\t\t\t\t展示的文字内容"
    },
    {
      "name": "property",
      "description": "{String | Number}\tduration\t\t\t展示时长，为0时不消失，单位ms ( 默认 3000 )"
    },
    {
      "name": "property",
      "description": "{String | Number}\tfontSize\t\t\t字体大小 ( 默认 15 )"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tsafeAreaInsetTop\t是否留出顶部安全距离（状态栏高度） ( 默认 false )"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tcustomStyle\t\t\t组件的样式，对象形式"
    },
    {
      "name": "event",
      "description": "{Function}\topen\t开启组件时调用的函数"
    },
    {
      "name": "event",
      "description": "{Function}\tclose\t关闭组件式调用的函数"
    },
    {
      "name": "example",
      "description": "<u-notify message=\"Hi uView\"></u-notify>"
    }
  ],
