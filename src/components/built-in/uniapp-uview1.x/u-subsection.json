  "keywords": [
    {
      "name": "description",
      "description": "该分段器一般用于用户从几个选项中选择某一个的场景"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/subsection.html"
    },
    {
      "name": "property",
      "description": "{Array} list 选项的数组，形式见上方\"基本使用\""
    },
    {
      "name": "property",
      "description": "{String Number} current 初始化时默认选中的选项索引值（默认0）"
    },
    {
      "name": "property",
      "description": "{String} active-color 激活时的颜色，mode为subsection时固定为白色（默认#303133）"
    },
    {
      "name": "property",
      "description": "{String} inactive-color 未激活时字体的颜色，mode为subsection时无效（默认#606266）"
    },
    {
      "name": "property",
      "description": "{String} mode 模式选择，见官网\"模式选择\"说明（默认button）"
    },
    {
      "name": "property",
      "description": "{String Number} font-size 字体大小，单位rpx（默认28）"
    },
    {
      "name": "property",
      "description": "{String Number} height 组件高度，单位rpx（默认70）"
    },
    {
      "name": "property",
      "description": "{Boolean} animation 是否开启动画效果，见上方说明（默认true）"
    },
    {
      "name": "property",
      "description": "{Boolean} bold 激活选项的字体是否加粗（默认true）"
    },
    {
      "name": "property",
      "description": "{String} bg-color 组件背景颜色，mode为button时有效（默认#eeeeef）"
    },
    {
      "name": "property",
      "description": "{String} button-color 按钮背景颜色，mode为button时有效（默认#ffffff）"
    },
    {
      "name": "event",
      "description": "{Function} change 分段器选项发生改变时触发"
    },
    {
      "name": "example",
      "description": "<u-subsection active-color=\"#ff9900\"></u-subsection>"
    }
  ],
