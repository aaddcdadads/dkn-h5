  "keywords": [
    {
      "name": "description",
      "description": "弹出层容器，用于展示弹窗、信息提示等内容，支持上、下、左、右和中部弹出。组件只提供容器，内部内容由用户自定义"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/popup.html"
    },
    {
      "name": "property",
      "description": "{String} mode 弹出方向（默认left）"
    },
    {
      "name": "property",
      "description": "{Boolean} mask 是否显示遮罩（默认true）"
    },
    {
      "name": "property",
      "description": "{Stringr | Number} length mode=left | 见官网说明（默认auto）"
    },
    {
      "name": "property",
      "description": "{Boolean} zoom 是否开启缩放动画，只在mode为center时有效（默认true）"
    },
    {
      "name": "property",
      "description": "{Boolean} safe-area-inset-bottom 是否开启底部安全区适配（默认false）"
    },
    {
      "name": "property",
      "description": "{Boolean} mask-close-able 点击遮罩是否可以关闭弹出层（默认true）"
    },
    {
      "name": "property",
      "description": "{Object} custom-style 用户自定义样式"
    },
    {
      "name": "property",
      "description": "{Stringr | Number} negative-top 中部弹出时，往上偏移的值"
    },
    {
      "name": "property",
      "description": "{Numberr | String} border-radius 弹窗圆角值（默认0）"
    },
    {
      "name": "property",
      "description": "{Numberr | String} z-index 弹出内容的z-index值（默认1075）"
    },
    {
      "name": "property",
      "description": "{Boolean} closeable 是否显示关闭图标（默认false）"
    },
    {
      "name": "property",
      "description": "{String} close-icon 关闭图标的名称，只能uView的内置图标"
    },
    {
      "name": "property",
      "description": "{String} close-icon-pos 自定义关闭图标位置（默认top-right）"
    },
    {
      "name": "property",
      "description": "{String} close-icon-color 关闭图标的颜色（默认#909399）"
    },
    {
      "name": "property",
      "description": "{Number | String} close-icon-size 关闭图标的大小，单位rpx（默认30）"
    },
    {
      "name": "event",
      "description": "{Function} open 弹出层打开"
    },
    {
      "name": "event",
      "description": "{Function} close 弹出层收起"
    },
    {
      "name": "example",
      "description": "<u-popup v-model=\"show\"><view>出淤泥而不染，濯清涟而不妖</view></u-popup>"
    }
  ],
