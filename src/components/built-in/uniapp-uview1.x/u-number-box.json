  "keywords": [
    {
      "name": "description",
      "description": "该组件一般用于商城购物选择物品数量的场景。注意：该输入框只能输入大于或等于0的整数，不支持小数输入"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/numberBox.html"
    },
    {
      "name": "property",
      "description": "{Number} value 输入框初始值（默认1）"
    },
    {
      "name": "property",
      "description": "{String} bg-color 输入框和按钮的背景颜色（默认#F2F3F5）"
    },
    {
      "name": "property",
      "description": "{Number} min 用户可输入的最小值（默认0）"
    },
    {
      "name": "property",
      "description": "{Number} max 用户可输入的最大值（默认99999）"
    },
    {
      "name": "property",
      "description": "{Number} step 步长，每次加或减的值（默认1）"
    },
    {
      "name": "property",
      "description": "{Boolean} disabled 是否禁用操作，禁用后无法加减或手动修改输入框的值（默认false）"
    },
    {
      "name": "property",
      "description": "{Boolean} disabled-input 是否禁止输入框手动输入值（默认false）"
    },
    {
      "name": "property",
      "description": "{Boolean} positive-integer 是否只能输入正整数（默认true）"
    },
    {
      "name": "property",
      "description": "{String | Number} size 输入框文字和按钮字体大小，单位rpx（默认26）"
    },
    {
      "name": "property",
      "description": "{String} color 输入框文字和加减按钮图标的颜色（默认#323233）"
    },
    {
      "name": "property",
      "description": "{String | Number} input-width 输入框宽度，单位rpx（默认80）"
    },
    {
      "name": "property",
      "description": "{String | Number} input-height 输入框和按钮的高度，单位rpx（默认50）"
    },
    {
      "name": "property",
      "description": "{String | Number} index 事件回调时用以区分当前发生变化的是哪个输入框"
    },
    {
      "name": "property",
      "description": "{Boolean} long-press 是否开启长按连续递增或递减(默认true)"
    },
    {
      "name": "property",
      "description": "{String | Number} press-time 开启长按触发后，每触发一次需要多久，单位ms(默认250)"
    },
    {
      "name": "property",
      "description": "{String | Number} cursor-spacing 指定光标于键盘的距离，避免键盘遮挡输入框，单位rpx（默认200）"
    },
    {
      "name": "event",
      "description": "{Function} change 输入框内容发生变化时触发，对象形式"
    },
    {
      "name": "event",
      "description": "{Function} blur 输入框失去焦点时触发，对象形式"
    },
    {
      "name": "event",
      "description": "{Function} minus 点击减少按钮时触发(按钮可点击情况下)，对象形式"
    },
    {
      "name": "event",
      "description": "{Function} plus 点击增加按钮时触发(按钮可点击情况下)，对象形式"
    },
    {
      "name": "example",
      "description": "<u-number-box :min=\"1\" :max=\"100\"></u-number-box>"
    }
  ],
