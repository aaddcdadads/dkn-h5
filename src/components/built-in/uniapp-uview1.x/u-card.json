    },
    {
      "name": "property",
      "description": "{Boolean} full 卡片与屏幕两侧是否留空隙（默认false）"
    },
    {
      "name": "property",
      "description": "{String} title 头部左边的标题"
    },
    {
      "name": "property",
      "description": "{String} title-color 标题颜色（默认#303133）"
    },
    {
      "name": "property",
      "description": "{String | Number} title-size 标题字体大小，单位rpx（默认30）"
    },
    {
      "name": "property",
      "description": "{String} sub-title 头部右边的副标题"
    },
    {
      "name": "property",
      "description": "{String} sub-title-color 副标题颜色（默认#909399）"
    },
    {
      "name": "property",
      "description": "{String | Number} sub-title-size 副标题字体大小（默认26）"
    },
    {
      "name": "property",
      "description": "{Boolean} border 是否显示边框（默认true）"
    },
    {
      "name": "property",
      "description": "{String | Number} index 用于标识点击了第几个卡片"
    },
    {
      "name": "property",
      "description": "{String} box-shadow 卡片外围阴影，字符串形式（默认none）"
    },
    {
      "name": "property",
      "description": "{String} margin 卡片与屏幕两边和上下元素的间距，需带单位，如\"30rpx 20rpx\"（默认30rpx）"
    },
    {
      "name": "property",
      "description": "{String | Number} border-radius 卡片整体的圆角值，单位rpx（默认16）"
    },
    {
      "name": "property",
      "description": "{Object} head-style 头部自定义样式，对象形式"
    },
    {
      "name": "property",
      "description": "{Object} body-style 中部自定义样式，对象形式"
    },
    {
      "name": "property",
      "description": "{Object} foot-style 底部自定义样式，对象形式"
    },
    {
      "name": "property",
      "description": "{Boolean} head-border-bottom 是否显示头部的下边框（默认true）"
    },
    {
      "name": "property",
      "description": "{Boolean} foot-border-top 是否显示底部的上边框（默认true）"
    },
    {
      "name": "property",
      "description": "{Boolean} show-head 是否显示头部（默认true）"
    },
    {
      "name": "property",
      "description": "{Boolean} show-head 是否显示尾部（默认true）"
    },
    {
      "name": "property",
      "description": "{String} thumb 缩略图路径，如设置将显示在标题的左边，不建议使用相对路径"
    },
    {
      "name": "property",
      "description": "{String | Number} thumb-width 缩略图的宽度，高等于宽，单位rpx（默认60）"
    },
    {
      "name": "property",
      "description": "{Boolean} thumb-circle 缩略图是否为圆形（默认false）"
    },
    {
      "name": "event",
      "description": "{Function} click 整个卡片任意位置被点击时触发"
    },
    {
      "name": "event",
      "description": "{Function} head-click 卡片头部被点击时触发"
    },
    {
      "name": "event",
      "description": "{Function} body-click 卡片主体部分被点击时触发"
    },
    {
      "name": "event",
      "description": "{Function} foot-click 卡片底部部分被点击时触发"
    },
