  "keywords": [
    {
      "name": "description",
      "description": "Button 按钮"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/button.html"
    },
    {
      "name": "property",
      "description": "{String} size 按钮的大小"
    },
    {
      "name": "property",
      "description": "{Boolean} ripple 是否开启点击水波纹效果"
    },
    {
      "name": "property",
      "description": "{String} ripple-bg-color 水波纹的背景色，ripple为true时有效"
    },
    {
      "name": "property",
      "description": "{String} type 按钮的样式类型"
    },
    {
      "name": "property",
      "description": "{Boolean} plain 按钮是否镂空，背景色透明"
    },
    {
      "name": "property",
      "description": "{Boolean} disabled 是否禁用"
    },
    {
      "name": "property",
      "description": "{Boolean} hair-line 是否显示按钮的细边框(默认true)"
    },
    {
      "name": "property",
      "description": "{Boolean} shape 按钮外观形状，见文档说明"
    },
    {
      "name": "property",
      "description": "{Boolean} loading 按钮名称前是否带 loading 图标(App-nvue 平台，在 ios 上为雪花，Android上为圆圈)"
    },
    {
      "name": "property",
      "description": "{String} form-type 用于 <form> 组件，点击分别会触发 <form> 组件的 submit/reset 事件"
    },
    {
      "name": "property",
      "description": "{String} open-type 开放能力"
    },
    {
      "name": "property",
      "description": "{String} data-name 额外传参参数，用于小程序的data-xxx属性，通过target.dataset.name获取"
    },
    {
      "name": "property",
      "description": "{String} hover-class 指定按钮按下去的样式类。当 hover-class=\"none\" 时，没有点击态效果(App-nvue 平台暂不支持)"
    },
    {
      "name": "property",
      "description": "{Number} hover-start-time 按住后多久出现点击态，单位毫秒"
    },
    {
      "name": "property",
      "description": "{Number} hover-stay-time 手指松开后点击态保留时间，单位毫秒"
    },
    {
      "name": "property",
      "description": "{Object} custom-style 对按钮的自定义样式，对象形式，见文档说明"
    },
    {
      "name": "event",
      "description": "{Function} click 按钮点击"
    },
    {
      "name": "event",
      "description": "{Function} getphonenumber open-type=\"getPhoneNumber\"时有效"
    },
    {
      "name": "event",
      "description": "{Function} getuserinfo 用户点击该按钮时，会返回获取到的用户信息，从返回参数的detail中获取到的值同uni.getUserInfo"
    },
    {
      "name": "event",
      "description": "{Function} error 当使用开放能力时，发生错误的回调"
    },
    {
      "name": "event",
      "description": "{Function} opensetting 在打开授权设置页并关闭后回调"
    },
    {
      "name": "event",
      "description": "{Function} launchapp 打开 APP 成功的回调"
    },
    {
      "name": "example",
      "description": "<u-button>月落</u-button>"
    }
  ],
