  "keywords": [
    {
      "name": "description",
      "description": "搜索组件，集成了常见搜索框所需功能，用户可以一键引入，开箱即用。"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/search.html"
    },
    {
      "name": "property",
      "description": "{String} shape 搜索框形状，round-圆形，square-方形（默认round）"
    },
    {
      "name": "property",
      "description": "{String} bg-color 搜索框背景颜色（默认#f2f2f2）"
    },
    {
      "name": "property",
      "description": "{String} border-color 边框颜色，配置了颜色，才会有边框"
    },
    {
      "name": "property",
      "description": "{String} placeholder 占位文字内容（默认“请输入关键字”）"
    },
    {
      "name": "property",
      "description": "{Boolean} clearabled 是否启用清除控件（默认true）"
    },
    {
      "name": "property",
      "description": "{Boolean} focus 是否自动获得焦点（默认false）"
    },
    {
      "name": "property",
      "description": "{Boolean} show-action 是否显示右侧控件（默认true）"
    },
    {
      "name": "property",
      "description": "{String} action-text 右侧控件文字（默认“搜索”）"
    },
    {
      "name": "property",
      "description": "{Object} action-style 右侧控件的样式，对象形式"
    },
    {
      "name": "property",
      "description": "{String} input-align 输入框内容水平对齐方式（默认left）"
    },
    {
      "name": "property",
      "description": "{Object} input-style 自定义输入框样式，对象形式"
    },
    {
      "name": "property",
      "description": "{Boolean} disabled 是否启用输入框（默认false）"
    },
    {
      "name": "property",
      "description": "{String} search-icon-color 搜索图标的颜色，默认同输入框字体颜色"
    },
    {
      "name": "property",
      "description": "{String} color 输入框字体颜色（默认#606266）"
    },
    {
      "name": "property",
      "description": "{String} placeholder-color placeholder的颜色（默认#909399）"
    },
    {
      "name": "property",
      "description": "{String} search-icon 输入框左边的图标，可以为uView图标名称或图片路径"
    },
    {
      "name": "property",
      "description": "{String} margin 组件与其他上下左右元素之间的距离，带单位的字符串形式，如\"30rpx\""
    },
    {
      "name": "property",
      "description": "{Boolean} animation 是否开启动画，见上方说明（默认false）"
    },
    {
      "name": "property",
      "description": "{String} value 输入框初始值"
    },
    {
      "name": "property",
      "description": "{String | Number} maxlength 输入框最大能输入的长度，-1为不限制长度"
    },
    {
      "name": "property",
      "description": "{Boolean} input-style input输入框的样式，可以定义文字颜色，大小等，对象形式"
    },
    {
      "name": "property",
      "description": "{String | Number} height 输入框高度，单位rpx（默认64）"
    },
    {
      "name": "event",
      "description": "{Function} change 输入框内容发生变化时触发"
    },
    {
      "name": "event",
      "description": "{Function} search 用户确定搜索时触发，用户按回车键，或者手机键盘右下角的\"搜索\"键时触发"
    },
    {
      "name": "event",
      "description": "{Function} custom 用户点击右侧控件时触发"
    },
    {
      "name": "event",
      "description": "{Function} clear 用户点击清除按钮时触发"
    },
    {
      "name": "example",
      "description": "<u-search placeholder=\"日照香炉生紫烟\" v-model=\"keyword\"></u-search>"
    }
  ],
