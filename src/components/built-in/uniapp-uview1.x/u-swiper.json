  "keywords": [
    {
      "name": "description",
      "description": "该组件一般用于导航轮播，广告展示等场景,可开箱即用"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/swiper.html"
    },
    {
      "name": "property",
      "description": "{Array} list 轮播图数据，见官网\"基本使用\"说明"
    },
    {
      "name": "property",
      "description": "{Boolean} title 是否显示标题文字，需要配合list参数，见官网说明（默认false）"
    },
    {
      "name": "property",
      "description": "{String} mode 指示器模式，见官网说明（默认round）"
    },
    {
      "name": "property",
      "description": "{String Number} height 轮播图组件高度，单位rpx（默认250）"
    },
    {
      "name": "property",
      "description": "{String} indicator-pos 指示器的位置（默认bottomCenter）"
    },
    {
      "name": "property",
      "description": "{Boolean} effect3d 是否开启3D效果（默认false）"
    },
    {
      "name": "property",
      "description": "{Boolean} autoplay 是否自动播放（默认true）"
    },
    {
      "name": "property",
      "description": "{String Number} interval 自动轮播时间间隔，单位ms（默认2500）"
    },
    {
      "name": "property",
      "description": "{Boolean} circular 是否衔接播放，见官网说明（默认true）"
    },
    {
      "name": "property",
      "description": "{String} bg-color 背景颜色（默认#f3f4f6）"
    },
    {
      "name": "property",
      "description": "{String Number} border-radius 轮播图圆角值，单位rpx（默认8）"
    },
    {
      "name": "property",
      "description": "{Object} title-style 自定义标题样式"
    },
    {
      "name": "property",
      "description": "{String Number} effect3d-previous-margin mode = true模式的情况下，激活项与前后项之间的距离，单位rpx（默认50）"
    },
    {
      "name": "property",
      "description": "{String} img-mode 图片的裁剪模式，详见image组件裁剪模式（默认aspectFill）"
    },
    {
      "name": "event",
      "description": "{Function} click 点击轮播图时触发"
    },
    {
      "name": "example",
      "description": "<u-swiper :list=\"list\" mode=\"dot\" indicator-pos=\"bottomRight\"></u-swiper>"
    }
  ],
