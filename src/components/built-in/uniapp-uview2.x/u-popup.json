  "keywords": [
    {
      "name": "description",
      "description": "弹出层容器，用于展示弹窗、信息提示等内容，支持上、下、左、右和中部弹出。组件只提供容器，内部内容由用户自定义"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/popup.html"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tshow\t\t\t\t是否展示弹窗 (默认 false )"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\toverlay\t\t\t\t是否显示遮罩 （默认 true ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tmode\t\t\t\t弹出方向（默认 'bottom' ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tduration\t\t\t动画时长，单位ms （默认 300 ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\toverlayDuration\t\t\t遮罩层动画时长，单位ms （默认 350 ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tcloseable\t\t\t是否显示关闭图标（默认 false ）"
    },
    {
      "name": "property",
      "description": "{Object | String}\toverlayStyle\t\t自定义遮罩的样式"
    },
    {
      "name": "property",
      "description": "{String | Number}\toverlayOpacity\t\t遮罩透明度，0-1之间（默认 0.5）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tcloseOnClickOverlay\t点击遮罩是否关闭弹窗 （默认  true ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tzIndex\t\t\t\t层级 （默认 10075 ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tsafeAreaInsetBottom\t是否为iPhoneX留出底部安全距离 （默认 true ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tsafeAreaInsetTop\t是否留出顶部安全距离（状态栏高度） （默认 false ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tcloseIconPos\t\t自定义关闭图标位置（默认 'top-right' ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tround\t\t\t\t圆角值（默认 0）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tzoom\t\t\t\t当mode=center时 是否开启缩放（默认 true ）"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tcustomStyle\t\t\t组件的样式，对象形式"
    },
    {
      "name": "event",
      "description": "{Function} open 弹出层打开"
    },
    {
      "name": "event",
      "description": "{Function} close 弹出层收起"
    },
    {
      "name": "example",
      "description": "<u-popup v-model=\"show\"><text>出淤泥而不染，濯清涟而不妖</text></u-popup>"
    }
  ],
