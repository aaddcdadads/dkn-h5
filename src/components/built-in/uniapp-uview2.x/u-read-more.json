  "keywords": [
    {
      "name": "description",
      "description": "该组件一般用于内容较长，预先收起一部分，点击展开全部内容的场景。"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/readMore.html"
    },
    {
      "name": "property",
      "description": "{String | Number}\tshowHeight\t内容超出此高度才会显示展开全文按钮，单位px（默认 400 ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\ttoggle\t\t展开后是否显示收起按钮（默认 false ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tcloseText\t关闭时的提示文字（默认 '展开阅读全文' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\topenText\t展开时的提示文字（默认 '收起' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tcolor\t\t提示文字的颜色（默认 '#2979ff' ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tfontSize\t提示文字的大小，单位px （默认 14 ）"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tshadowStyle\t显示阴影的样式"
    },
    {
      "name": "property",
      "description": "{String}\t\t\ttextIndent\t段落首行缩进的字符个数 （默认 '2em' ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tname\t\t用于在 open 和 close 事件中当作回调参数返回"
    },
    {
      "name": "event",
      "description": "{Function} open 内容被展开时触发"
    },
    {
      "name": "event",
      "description": "{Function} close 内容被收起时触发"
    },
    {
      "name": "example",
      "description": "<u-read-more><rich-text :nodes=\"content\"></rich-text></u-read-more>"
    }
  ],
