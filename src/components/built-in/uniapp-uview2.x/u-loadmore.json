{
  "inheritAttrs": true,
  "errors": [],
  "warnings": [],
  "keywords": [
    {
      "name": "description",
      "description": "此组件一般用于标识页面底部加载数据时的状态。"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/loadMore.html"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tstatus\t\t\t组件状态（默认 'loadmore' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tbgColor\t\t\t组件背景颜色，在页面是非白色时会用到（默认 'transparent' ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\ticon\t\t\t加载中时是否显示图标（默认 true ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tfontSize\t\t字体大小（默认 14 ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\ticonSize\t\t图标大小（默认 17 ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tcolor\t\t\t字体颜色（默认 '#606266' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tloadingIcon\t\t加载图标（默认 'circle' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tloadmoreText\t加载前的提示语（默认 '加载更多' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tloadingText\t\t加载中提示语（默认 '正在加载...' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tnomoreText\t\t没有更多的提示语（默认 '没有更多了' ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tisDot\t\t\t到上一个相邻元素的距离 （默认 false ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\ticonColor\t\t加载中图标的颜色 （默认 '#b7b7b7' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tlineColor\t\t线条颜色（默认 #E6E8EB ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tmarginTop\t\t上边距 （默认 10 ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tmarginBottom\t下边距 （默认 10 ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\theight\t\t\t高度，单位px （默认 'auto' ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tline\t\t\t是否显示左边分割线  （默认 false ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tdashed\t\t// 是否虚线，true-虚线，false-实线  （默认 false ）"
    },
    {
      "name": "event",
      "description": "{Function} loadmore status为loadmore时，点击组件会发出此事件"
    },
    {
      "name": "example",
      "description": "<u-loadmore :status=\"status\" icon-type=\"iconType\" load-text=\"loadText\" />"
    }
  ],
