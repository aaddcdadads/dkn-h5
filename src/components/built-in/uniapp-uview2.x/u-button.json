{
  "inheritAttrs": true,
  "errors": [],
  "warnings": [],
  "keywords": [
    {
      "name": "description",
      "description": "Button 按钮"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/button.html"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\thairline\t\t\t\t是否显示按钮的细边框 (默认 true )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\ttype\t\t\t\t\t按钮的预置样式，info，primary，error，warning，success (默认 'info' )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tsize\t\t\t\t\t按钮尺寸，large，normal，mini （默认 normal）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tshape\t\t\t\t\t按钮形状，circle（两边为半圆），square（带圆角） （默认 'square' ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tplain\t\t\t\t\t按钮是否镂空，背景色透明 （默认 false）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tdisabled\t\t\t\t是否禁用 （默认 false）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tloading\t\t\t\t\t按钮名称前是否带 loading 图标(App-nvue 平台，在 ios 上为雪花，Android上为圆圈) （默认 false）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tloadingText\t\t\t\t加载中提示文字"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tloadingMode\t\t\t\t加载状态图标类型 （默认 'spinner' ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\tloadingSize\t\t\t\t加载图标大小 （默认 15 ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\topenType\t\t\t\t开放能力，具体请看uniapp稳定关于button组件部分说明"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tformType\t\t\t\t用于 <form> 组件，点击分别会触发 <form> 组件的 submit/reset 事件"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tappParameter\t\t\t打开 APP 时，向 APP 传递的参数，open-type=launchApp时有效 （注：只微信小程序、QQ小程序有效）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\thoverStopPropagation\t指定是否阻止本节点的祖先节点出现点击态，微信小程序有效（默认 true ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tlang\t\t\t\t\t指定返回用户信息的语言，zh_CN 简体中文，zh_TW 繁体中文，en 英文（默认 en ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tsessionFrom\t\t\t\t会话来源，openType=\"contact\"时有效"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tsendMessageTitle\t\t会话内消息卡片标题，openType=\"contact\"时有效"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tsendMessagePath\t\t\t会话内消息卡片点击跳转小程序路径，openType=\"contact\"时有效"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tsendMessageImg\t\t\t会话内消息卡片图片，openType=\"contact\"时有效"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tshowMessageCard\t\t\t是否显示会话内消息卡片，设置此参数为 true，用户进入客服会话会在右下角显示\"可能要发送的小程序\"提示，用户点击后可以快速发送小程序消息，openType=\"contact\"时有效（默认false）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tdataName\t\t\t\t额外传参参数，用于小程序的data-xxx属性，通过target.dataset.name获取"
    },
    {
      "name": "property",
      "description": "{String | Number}\tthrottleTime\t\t\t节流，一定时间内只能触发一次 （默认 0 )"
    },
    {
      "name": "property",
      "description": "{String | Number}\thoverStartTime\t\t\t按住后多久出现点击态，单位毫秒 （默认 0 )"
    },
    {
      "name": "property",
      "description": "{String | Number}\thoverStayTime\t\t\t手指松开后点击态保留时间，单位毫秒 （默认 200 )"
    },
    {
      "name": "property",
      "description": "{String | Number}\ttext\t\t\t\t\t按钮文字，之所以通过props传入，是因为slot传入的话（注：nvue中无法控制文字的样式）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\ticon\t\t\t\t\t按钮图标"
    },
    {
      "name": "property",
      "description": "{String}\t\t\ticonColor\t\t\t\t按钮图标颜色"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tcolor\t\t\t\t\t按钮颜色，支持传入linear-gradient渐变色"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tcustomStyle\t\t\t\t定义需要用到的外部样式"
    },
    {
      "name": "event",
      "description": "{Function}\tclick\t\t\t非禁止并且非加载中，才能点击"
    },
    {
      "name": "event",
      "description": "{Function}\tgetphonenumber\topen-type=\"getPhoneNumber\"时有效"
    },
    {
      "name": "event",
      "description": "{Function}\tgetuserinfo\t\t用户点击该按钮时，会返回获取到的用户信息，从返回参数的detail中获取到的值同uni.getUserInfo"
    },
    {
      "name": "event",
      "description": "{Function}\terror\t\t\t当使用开放能力时，发生错误的回调"
    },
    {
      "name": "event",
      "description": "{Function}\topensetting\t\t在打开授权设置页并关闭后回调"
    },
    {
      "name": "event",
      "description": "{Function}\tlaunchapp\t\t打开 APP 成功的回调"
    },
    {
      "name": "example",
      "description": "<u-button>月落</u-button>"
    }
  ],
