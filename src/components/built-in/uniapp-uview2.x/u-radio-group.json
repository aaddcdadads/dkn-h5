      "description": "{String}\t\t\t\t\t\tname\t\t\t标识符"
    },
    {
      "name": "property",
      "description": "{String | Number}\t\t\t\tsize\t\t\t组件整体的大小，单位px（默认 18 ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\t\t\t\tplacement\t\t布局方式，row-横向，column-纵向 （默认 'row' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\t\t\t\tlabel\t\t\t文本"
    },
    {
      "name": "property",
      "description": "{String}\t\t\t\t\t\tlabelColor\t\tlabel的颜色 （默认 '#303133' ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\t\t\t\tlabelSize\t\tlabel的字体大小，px单位 （默认 14 ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\t\t\t\tlabelDisabled\t是否禁止点击文本操作checkbox(默认 false )"
    },
    {
      "name": "property",
      "description": "{String}\t\t\t\t\t\ticonColor\t\t图标颜色 （默认 '#ffffff' ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\t\t\t\ticonSize\t\t图标的大小，单位px （默认 12 ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\t\t\t\tborderBottom\tplacement为row时，是否显示下边框 （默认 false ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\t\t\t\ticonPlacement\t图标与文字的对齐方式 （默认 'left' ）"
    },
