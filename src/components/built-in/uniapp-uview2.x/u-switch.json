    },
    {
      "name": "property",
      "description": "{String} \t\t\t\t\t\tinactiveColor\t关闭时的背景色 （默认 '#ffffff' ）"
    },
    {
      "name": "property",
      "description": "{Boolean | String | Number}\tvalue\t\t\t通过v-model双向绑定的值 （默认 false ）"
    },
    {
      "name": "property",
      "description": "{Boolean | String | Number}\tactiveValue\t\t打开选择器时通过change事件发出的值 （默认 true ）"
    },
    {
      "name": "property",
      "description": "{Boolean | String | Number}\tinactiveValue\t关闭选择器时通过change事件发出的值 （默认 false ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\t\t\t\tasyncChange\t\t是否开启异步变更，开启后需要手动控制输入值 （默认 false ）"
    },
    {
      "name": "property",
      "description": "{String | Number}\t\t\t\tspace\t\t\t圆点与外边框的距离 （默认 0 ）"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\t\t\t\tcustomStyle\t\t定义需要用到的外部样式"
    },
    {
      "name": "event",
      "description": "{Function} change 在switch打开或关闭时触发"
    },
    {
      "name": "example",
      "description": "<u-switch v-model=\"checked\" active-color=\"red\" inactive-color=\"#eee\"></u-switch>"
    }
