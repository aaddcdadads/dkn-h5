  "keywords": [
    {
      "name": "description",
      "description": "该组件一般使用于某个活动的截止时间上，通过数字的变化，给用户明确的时间感受，提示用户进行某一个行为操作。"
    },
    {
      "name": "tutorial",
      "description": "https://uviewui.com/components/countDown.html"
    },
    {
      "name": "property",
      "description": "{String | Number}\ttime\t\t倒计时时长，单位ms （默认 0 ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tformat\t\t时间格式，DD-日，HH-时，mm-分，ss-秒，SSS-毫秒  （默认 'HH:mm:ss' ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tautoStart\t是否自动开始倒计时 （默认 true ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tmillisecond\t是否展示毫秒倒计时 （默认 false ）"
    },
    {
      "name": "event",
      "description": "{Function} finish 倒计时结束时触发"
    },
    {
      "name": "event",
      "description": "{Function} change 倒计时变化时触发"
    },
    {
      "name": "event",
      "description": "{Function} start\t开始倒计时"
    },
    {
      "name": "event",
      "description": "{Function} pause\t暂停倒计时"
    },
    {
      "name": "event",
      "description": "{Function} reset\t重设倒计时，若 auto-start 为 true，重设后会自动开始倒计时"
    },
    {
      "name": "example",
      "description": "<u-count-down :time=\"time\"></u-count-down>"
    }
  ],
