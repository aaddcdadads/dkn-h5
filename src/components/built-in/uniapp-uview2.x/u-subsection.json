{
  "inheritAttrs": true,
  "errors": [],
  "warnings": [],
  "keywords": [
    {
      "name": "description",
      "description": "该分段器一般用于用户从几个选项中选择某一个的场景"
    },
    {
      "name": "tutorial",
      "description": "https://www.uviewui.com/components/subsection.html"
    },
    {
      "name": "property",
      "description": "{Array}\t\t\tlist\t\t\ttab的数据"
    },
    {
      "name": "property",
      "description": "{String ｜ Number}\tcurrent\t\t\t当前活动的tab的index（默认 0 ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tactiveColor\t\t激活时的颜色（默认 '#3c9cff' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tinactiveColor\t未激活时的颜色（默认 '#303133' ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tmode\t\t\t模式选择，mode=button为按钮形式，mode=subsection时为分段模式（默认 'button' ）"
    },
    {
      "name": "property",
      "description": "{String ｜ Number}\tfontSize\t\t字体大小，单位px（默认 12 ）"
    },
    {
      "name": "property",
      "description": "{Boolean}\t\t\tbold\t\t\t激活选项的字体是否加粗（默认 true ）"
    },
    {
      "name": "property",
      "description": "{String}\t\t\tbgColor\t\t\t组件背景颜色，mode为button时有效（默认 '#eeeeef' ）"
    },
    {
      "name": "property",
      "description": "{Object}\t\t\tcustomStyle\t\t定义需要用到的外部样式"
    },
    {
      "name": "property",
      "description": "{String}\t        keyName\t        从`list`元素对象中读取的键名（默认 'name' ）"
    },
    {
      "name": "event",
      "description": "{Function} change\t\t分段器选项发生改变时触发  回调 index：选项的index索引值，从0开始"
    },
    {
      "name": "example",
      "description": "<u-subsection :list=\"list\" :current=\"curNow\" @change=\"sectionChange\"></u-subsection>"
    }
  ],
